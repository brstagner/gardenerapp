{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["username", "password", "email", "location"],
  "additionalProperties": false,
  "properties": {
    "username": {
      "type": "string",
      "default": "",
      "title": "The username Schema",
      "examples": ["schemaUser"]
    },
    "password": {
      "type": "string",
      "default": "",
      "title": "The password Schema",
      "examples": ["schemaPass"]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": ["email@email.com"]
    },
    "location": {
      "type": "JSON",
      "default": "",
      "title": "The location Schema",
      "examples": [{ "name": "Alaska" }]
    },
    "is_admin": {
      "type": "boolean",
      "default": "",
      "title": "The is_adminSchema",
      "examples": ["true"]
    }
  },
  "examples": [
    {
      "username": "schemaUser",
      "password": "schemaPass",
      "email": "email@email.com",
      "location": "Alaska"
    }
  ]
}
